extends shared

block content
 div.container
   br 
   br
   h1 Questionnaire List  (#{gametype.split("_")[1]})
   br


   form.scrol(action="/activeQuestionnaire", method="post")
     table(class="table table-striped table-bordered")
       tr
         th time
         th Name
         th Gender
         th Age
         th Education
         th Field
         th Birth Country
         th browser Type
         th user id
         th gametype
         th country
         th role
         th gameid
         th assisnmentid
         th hitid
         th workerid
         th datastatus
         th computer_program
         th fairness
         th feedback
         th happy
         th instruction
         th otherGender
         th same_country
         th type
         th Completed
         th Active
         th deleteMe
       - if (questionnaireList !== undefined) 
         each questionnaire in questionnaireList
          if (questionnaire.gametype == gametype || questionnaire.gametype == gametype.split("_")[0])
             tr
               td #{questionnaire.Timestamp}
               td #{questionnaire.yourname}
               td #{questionnaire.gender}
               td #{questionnaire.age}
               td #{questionnaire.education}
               td #{questionnaire.field}
               td #{questionnaire.birth_country}
               td #{questionnaire.browserType}
               td #{questionnaire.userid}
               td #{questionnaire.gametype}
               td #{questionnaire.country}
               td #{questionnaire.role}
               td #{questionnaire.gameid} 
               td #{questionnaire.assignmentId}
               td #{questionnaire.hitId}
               td #{questionnaire.workerId}
               td #{questionnaire.datastatus}
               td #{questionnaire.computer_program}
               td #{questionnaire.fairness}
               td #{questionnaire.feedback}
               td #{questionnaire.happy}
               td #{questionnaire.instruction}
               td #{questionnaire.otherGender}
               td #{questionnaire.same_country}
               td #{questionnaire.type}
               td #{questionnaire.completed}
               td
                 input(type="hidden", name="#{questionnaire.RowKey}", value="#{questionnaire.RowKey}")
                 input(type="checkbox", name="active", value="#{!questionnaire.active}", checked=questionnaire.active)
               td 
                 a(href= "/#{questionnaire.gametype},#{questionnaire.RowKey},#{questionnaire.PartitionKey}/deleteQuestionnaire") deleteMe 
                  
     