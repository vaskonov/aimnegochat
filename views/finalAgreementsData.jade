extends shared

block content
 div.container
   br 
   br
   h1= title
   br


   form.scrol(action="/activeGames", method="post")
     table(class="table table-striped table-bordered")
       tr
         th Game Id
         th Issue
         th Value
         th country
         th Delete
       - if (FinalAgreementList !== undefined) 
         each FinalResult in FinalAgreementList
          tr 
              td #{FinalResult.PartitionKey}
              td #{FinalResult.issue}
              td #{FinalResult.value}
              td #{FinalResult.country}
              td 
                a(href= "/#{gametype},#{FinalResult.RowKey},#{FinalResult.PartitionKey}/deleteFinalAgreements") deleteMe 
                