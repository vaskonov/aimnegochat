  body(class="#{accountName}") 
    .navbar.navbar-inverse.navbar-fixed-top
      .navbar-inner
       .container
          button(type="button", class="btn btn-navbar", data-toggle="collapse", data-target=".nav-collapse")
           span.icon-bar
           span.icon-bar
           span.icon-bar
          a.brand(href='#') Chat Project
          .nav-collapse.collapse
              ul.nav
                li
                  a(href='/') Home

                li
                  a(href='#about') About
                li.dropdown
                  a.dropdown-toggle(href='#', data-toggle='dropdown')
                    | Play
                    b.caret
                  ul.dropdown-menu
                   for game in domains
                    li
                      a(href='/#{game}/chooseGame') #{game}
                for type in Object.keys(gametypes)
                  if (type == gametype.split("_")[0]) 
                    li.dropdown.active
                      a.dropdown-toggle(href='#', data-toggle='dropdown')
                        | #{type}
                        b.caret
                      ul.dropdown-menu
                       for t in gametypes[type]
                        li
                          a(href='/#{type+'_'+t}/gametypeCountry') #{t}
                  else
                    li.dropdown
                      a.dropdown-toggle(href='#', data-toggle='dropdown')
                        | #{type}
                        b.caret
                      ul.dropdown-menu
                       for t in gametypes[type]
                        li
                          a(href='/#{type+'_'+t}/gametypeCountry') #{t}
                          

